.submitBox{
  width: 700rpx;
  margin: 10rpx auto;
}
.tit{
  font-size: 32rpx;
  font-weight: bold;
}
.imgList{
  width: 700rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.imgWrap{
  width: 120rpx;
  height: 120rpx;
  margin-top: 20rpx;
  margin-right: 20rpx;
  position: relative;
  overflow:visible;
}
.imgItem{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: -15rpx;
  right: -15rpx;
  z-index: 999;
  
}
.ImgBlock{
  width: 120rpx;
  height: 120rpx;
  margin-top: 20rpx;
  margin-right: 20rpx;
}