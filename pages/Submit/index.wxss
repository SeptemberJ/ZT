.submitBox{
  width: 700rpx;
  margin: 10rpx auto;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: flex-start;
  padding: 10px auto 20px auto;
}
.tit{
  width: 100%;
  font-size: 32rpx;
  font-weight: bold;
  margin: 15rpx auto;
}
.titWrap{
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.imgList{
  width: 700rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}
.imgWrap{
  width: 120rpx;
  height: 120rpx;
  margin-top: 20rpx;
  margin-right: 20rpx;
  position: relative;
  overflow:visible;
}
.imgItem{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  top: -15rpx;
  right: -15rpx;
  z-index: 999;
  
}
.ImgBlock{
  width: 120rpx;
  height: 120rpx;
  margin-top: 20rpx;
  margin-right: 20rpx;
}
textarea{
  width: calc(700rpx - 40rpx);
  border: 1rpx solid #eee;
  min-height: 200rpx;
  font-size: 32rpx;
  padding: 20rpx;
}
input{
  width: 100%;
  font-size: 32rpx;
  padding: 20rpx;
}
.Qcode{
  width: 60rpx;
  height: 60rpx;
}